{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/search_text",
      "dest": "main.py"
    },
    {
      "src": "/api/search",
      "dest": "main.py"
    },
    {
      "src": "/images/(.*)",
      "dest": "main.py"
    },
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ]
}